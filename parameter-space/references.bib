@article{altweggCometaryChemistryOrigin2019,
  title = {Cometary {{Chemistry}} and the {{Origin}} of {{Icy Solar System Bodies}}: {{The View After Rosetta}}},
  shorttitle = {Cometary {{Chemistry}} and the {{Origin}} of {{Icy Solar System Bodies}}},
  author = {Altwegg, Kathrin and Balsiger, Hans and Fuselier, Stephen A.},
  year = {2019},
  journal = {Annual Review of Astronomy and Astrophysics},
  volume = {57},
  number = {1},
  pages = {113--155},
  doi = {10.1146/annurev-astro-091918-104409},
  abstract = {In situ research of cometary chemistry began when measurements from the Giotto mission at Comet 1P/Halley revealed the presence of complex organics in the coma. New telescopes and space missions have provided detailed remote and in situ measurements of the composition of cometary volatiles. Recently, the Rosetta mission to Comet 67P/Churyumov\textendash Gerasimenko (67P) more than doubled the number of parent species and the number of isotopic ratios known for comets. Forty of the 71 parent species have also been detected in pre- and protostellar clouds. Most isotopic ratios are nonsolar. This diverse origin is in contrast to that of the Sun, which received its material from the bulk of the collapsing cloud. The xenon isotopic ratios measured in 67P can explain the long-standing question about the origin of terrestrial atmospheric xenon. These findings strengthen the notion that comets are indeed an important link between the ISM and today's solar system including life on Earth. {$\blacksquare$} Nonsolar isotopic ratios for species such as Xe, N, S, and Si point to a nonhomogenized protoplanetary disk from which comets received their material. {$\blacksquare$} The similarity of the organic inventories of comets and presolar and protostellar material makes it plausible that this material was accreted almost unaltered by comets from the presolar stage. {$\blacksquare$} Large variations in the deuterium-to-hydrogen ratio in water for comets indicate a large range in the protoplanetary disk from which comets formed. {$\blacksquare$} The amount of organics delivered by comets to Earth may be highly significant.},
  keywords = {astrochemistry,Comet 67P/Churyumov–Gerasimenko,cometary molecules,exoplanet-atmosphere,interstellar molecules,isotopologues},
  annotation = {\_eprint: https://doi.org/10.1146/annurev-astro-091918-104409},
  file = {/Users/py13je/Zotero/storage/NJCRJJ2P/Altwegg et al. - 2019 - Cometary Chemistry and the Origin of Icy Solar Sys.pdf}
}

