  title = {Stellar {{Evolution Physics}}, {{Volume}} 2: {{Advanced Evolution}} of {{Single Stars}}},
  shorttitle = {Stellar {{Evolution Physics}}, {{Volume}} 2},
  author = {Iben, Jr., Icko},
  year = {2013},
  month = jan,
