  author = {Hamaguchi, Kenji and Corcoran, Michael F. and Pittard, Julian M. and Sharma, Neetika and Takahashi, Hiromitsu and Russell, Christopher M. P. and Grefenstette, Brian W. and Wik, Daniel R. and Gull, Theodore R. and Richardson, Noel D. and Madura, Thomas I. and Moffat, Anthony F. J.},
