@article{shenarWolfRayetBinaries2019,
  title = {The {{Wolf}}\textendash{{Rayet}} Binaries of the Nitrogen Sequence in the {{Large Magellanic Cloud}}: {{Spectroscopy}}, Orbital Analysis, Formation, and Evolution},
  shorttitle = {The {{Wolf}}\textendash{{Rayet}} Binaries of the Nitrogen Sequence in the {{Large Magellanic Cloud}}},
  author = {Shenar, T. and Sablowski, D. P. and Hainich, R. and Todt, H. and Moffat, A. F. J. and Oskinova, L. M. and Ramachandran, V. and Sana, H. and Sander, A. A. C. and Schnurr, O. and {St-Louis}, N. and Vanbeveren, D. and G{\"o}tberg, Y. and Hamann, W.-R.},
